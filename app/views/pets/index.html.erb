<h1>All Pets</h1>

<% @pets.each do |pet| %>
  <h2><%= pet.name %> </h2>
  <p><%= "#{pet.image}" %></p>
  <ul>
    <li><%= "Age: #{pet.approximate_age}" %></li>
    <li><%= "Sex: #{pet.sex}" %></li>
    <li><%= "Shelter: #{pet.shelter.name}" %></li>
    <li><%= "Description: #{pet.description}" %></li>

  </ul>

<% end %>
