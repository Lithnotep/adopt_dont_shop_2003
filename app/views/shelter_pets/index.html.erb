<<h1><%= @shelter.name %></h1>

<%= link_to "New Artist", "/artists/new" %>
<% @shelter.pets.each do |pet| %>
  <h2><%= pet.name %> </h2>
  <p><%= "#{pet.image}" %></p>
  <ul>
    <li><%= "Age: #{pet.approximate_age}" %></li>
    <li><%= "Sex: #{pet.sex}" %></li>
    <li><%= "Shelter: #{pet.description}" %></li>
  </ul>
  <% end %>
<%= link_to "Create Pet", "/pets/new" %>
